    <script>
        let x=document.getElementById("id1")
        console.log(x)
        let y=document.getElementsByClassName("a")
        console.log(y)
        for(let j=0;j<y.length;j++){
            console.log(y[j])
        }
        let z=document.getElementsByTagName("h1")
        console.log(z)
        for(let i=0;i<z.length;i++){
            console.log(z[i])
        }
        let b=document.querySelector('h1');
        console.log(b)
        let bb=document.querySelectorAll('h1');
        console.log(bb)

        let t=document.createElement("h2");
        t.className='tt'
        t.textContent="jyoshna"
        t.color="pink"
        document.write(t)        
        const v=["ammulu","josh","joshna"]
        for(let k=0;k<v.length;k++){
            let h=document.createElement("h3");
            h.className="aa"
            h.textContent=v[k];
            h.color="green";
            console.log(h)
        }
        let title;
        for(let p=0;p<3;p++){
            title=document.createElement("h1");
            title.className='p'
            title.style.fontSize='13px'
            title.textContent=p;
            console.log(p)
            document.write(p)
        }
    </script>
